{
    "name": "mngez/nova-opening-hours-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "replace": {
        "sadekd/nova-opening-hours-field":"*"
    },
    "autoload": {
        "psr-4": {
            "Sadekd\\NovaOpeningHoursField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sadekd\\NovaOpeningHoursField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
